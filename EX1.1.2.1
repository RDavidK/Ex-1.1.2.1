        //Start
      Scanner input = new Scanner(System.in);
       System.out.println("Enter numbers");
        
        int number1 = input.nextInt();
        
        int number2 = input.nextInt();
        
        int sum;
        
        sum = number1 + number2;
        
        System.out.printf("Sum is %d\n", sum );
    }//Stop
}
